#content{
	margin-top: 20px;
}

#header, .top-header .logo{
	background-color: #17202A;
}

.help-danger{
	color: #E94B3B;
}

.card-view .title{
	width: 50% !important;
}

.card-view .value{
	width: 50% !important;
}

.modal{
	z-index: 9998;
	overflow-y: scroll;
}

.ui-autocomplete{
	z-index: 9999 !important;
	position: absolute;
	width: 250px;
	list-style-type: none;
}


.ui-widget {
	font-family: "Helvetica Neue","Trebuchet MS",Tahoma,Verdana,Arial,sans-serif;
	background: #29a2d9;
	border: none;
	border-radius: 0;
	-webkit-border-radius: 0;
	-moz-border-radius: 0;
	box-shadow: 0 1px 3px #252525;
}

.btn-transparent{
	background-color: rgba(255, 255, 255, 0.5);
	border-color: none;
}

.ui-menu{
	background-color: #FFF;
}

.ui-menu-item{
	padding: 5px;
}

.ui-menu-item:hover{
	background-color: #17202A;
	color: #fff;
}

.popover{
	z-index: 99999;
}

.order-column:hover{
	background-color: #17202A;
	color: white;
	cursor: pointer;
}

.order-column-active{
	background-color: #17202A;
	color: white;
	cursor: pointer;
}
