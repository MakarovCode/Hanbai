<% array.each do |object| %>
<h3>
  <small>
    <i class="<%= object[:icon]%>"></i>
    <b><%= object[:count]%></b> <%= object[:name]%>
  </small>
  <small class="pull-right">
    <%= object[:count_difference]%>
    <% if object[:count_difference] > 0 %>
    <i class="fa fa-chevron-up" style="color: #23AE89;"></i>
    <% elsif object[:count_difference] < 0 %>
    <i class="fa fa-chevron-down" style="color: #d92a18;"></i>
    <% else %>
    <i class="fa fa-minus"></i>
    <% end %>
  </small>
</h3>
<div class="progress">
  <div class="progress-bar <%= type %>" role="progressbar" aria-valuenow="<%= object[:count] * 100 / max.to_f %>" aria-valuemin="0" aria-valuemax="100" style="width: <%= object[:count] * 100 / max.to_f %>%;">
    <span class="sr-only">-</span>
  </div>
</div>
<% end %>
<% if  array.count == 0%>
<div class="alert alert-info">
  No hay suficientes registros.
</div>
<% end %>
